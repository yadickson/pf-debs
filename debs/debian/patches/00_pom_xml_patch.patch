--- pf-5.5.0/pom.xml	1969-12-31 20:00:00.000000000 -0400
+++ pf-5.5.0~/pom.xml	2015-06-22 10:15:29.058081878 -0430
@@ -0,0 +1,55 @@
+<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
+
+  <modelVersion>4.0.0</modelVersion>
+
+  <groupId>org.pf</groupId>
+  <artifactId>pf</artifactId>
+  <packaging>jar</packaging>
+  <name>Programmer's Friend</name>
+  <description>This is a large collection of Java utility classes that are supposed to to simplify many things that are quite cumbersome if you have to use just the JDK.</description>
+  <version>5.5.0</version>
+
+  <licenses>
+    <license>
+      <name>Eclipse Public License - v 1.0</name>
+      <url>http://www.programmers-friend.org/epl-v10.html</url>
+      <distribution>repo</distribution>
+    </license>
+  </licenses>
+
+  <properties>
+    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
+  </properties>
+
+  <build>
+    <directory>target</directory>
+    <finalName>${artifactId}-${version}</finalName>
+    <sourceDirectory>src</sourceDirectory>
+    <plugins>
+      <plugin>
+        <groupId>org.apache.maven.plugins</groupId>
+        <artifactId>maven-compiler-plugin</artifactId>
+        <version>2.5.1</version>
+        <configuration>
+          <showDeprecation>true</showDeprecation>
+          <showWarnings>true</showWarnings>
+          <source>1.5</source>
+          <target>1.5</target>
+        </configuration>
+      </plugin>
+      <plugin>
+        <groupId>org.apache.maven.plugins</groupId>
+        <artifactId>maven-jar-plugin</artifactId>
+        <configuration>
+          <archive>
+            <manifest>
+              <classpathPrefix>/usr/share/maven-repo/</classpathPrefix>
+              <classpathLayoutType>repository</classpathLayoutType>
+            </manifest>
+          </archive>
+        </configuration>
+        <version>2.4</version>
+      </plugin>
+    </plugins>
+  </build>
+</project>
